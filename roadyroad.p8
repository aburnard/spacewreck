pico-8 cartridge // http://www.pico-8.com
version 30
__lua__
function _init()
turn_number=0
fog=false
current_player=1
cpx=5
cpy=5
map_setup()
make_player()
make_player2()
player_state=1
init_timer()
end

function _update()
 update_timer()
 sum_of_moves=p.moves+p2.moves
 if (player_state==1) player_1_state()
 if (player_state==2) player_2_state()
 
 
 if (btnp(‚ùé)) toggle_turn()
 if (sum_of_moves<=0) new_turn()
 
 --if (sum_0f_moves==0) 
end

function _draw()
cls()
draw_map()
draw_fog()
--draw_current_player()
draw_player()
draw_player2()
display_debug()
end

function new_turn()
	grow_berries()
 p.moves=p.start_moves
 p2.moves=p2.start_moves
 turn_number+=1
 
end
--expanding pool
function toggle_turn()
  if (player_state==1) then
   player_state=2
   else player_state=1
  end
end
