--fog code

function draw_fog()

if (fog) then
 fogx=flr(cpx/16)*16
 fogy=flr(cpy/16)*16
 camera(fogx*8,fogy*8)

 --map(0,0,0,0,128,64)

 map(64,0,0,0,128,64)
 
end

end

function lift_fog_0ne()
 mset(p.x+63,p.y-1,65)
 mset(p.x+64,p.y-1,65)
 mset(p.x+65,p.y-1,65)
 mset(p.x+63,p.y,65)
 mset(p.x+64,p.y,65)
 mset(p.x+65,p.y,65)
 mset(p.x+63,p.y+1,65)
 mset(p.x+64,p.y+1,65)
 mset(p.x+65,p.y+1,65)
end

function lift_fog_two()
 mset(p2.x+63,p2.y-1,65)
 mset(p2.x+64,p2.y-1,65)
 mset(p2.x+65,p2.y-1,65)
 mset(p2.x+63,p2.y,65)
 mset(p2.x+64,p2.y,65)
 mset(p2.x+65,p2.y,65)
 mset(p2.x+63,p2.y+1,65)
 mset(p2.x+64,p2.y+1,65)
 mset(p2.x+65,p2.y+1,65)
end
