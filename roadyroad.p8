--plants
function grow_berries()
 
 for x=0,128 do
  for y=0,128 do
   
   if (is_tile(space_berry,x,y)) then
    
    for growx=0,2 do
    for growy=0,2 do
    if (flr(rnd(5)+1)==2) then
    
    if (not is_tile(road,x+growx-1,y+growy-1))then
    
    mset(x+growx-1,y+growy-1,22)
    --mset(x,y-1,22)
    --mset(x+1,y-1,22)
    end
    end
    end
    end
    
   end
  
  end
 end
 
 for x=0,128 do
  for y=0,128 do
   
   if (is_tile(new_growth,x,y)) then
    mset(x,y,21)
    
   end
  
  end
 end
 
 
 
 
 
end
