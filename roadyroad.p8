--player code
function make_player()
  p={}
  p.x=10
  p.y=4 
  p.sprite=12
  p.moves=2
  p.start_moves=2
end

function make_player2()
  p2={}
  p2.x=12
  p2.y=4
  p2.sprite=14
  p2.moves=2
  p2.start_moves=2
end

function draw_player()
 
 lift_fog_0ne()
 if player_state==1 then
 cpx=p.x
 cpy=p.y
 if get_timer()>10 then
 spr(p.sprite,p.x*8,p.y*8)
 end
 end
 if player_state==2 then
  spr(p.sprite,p.x*8,p.y*8)
 end
 
end

function draw_player2()

lift_fog_two()
if player_state==2 then
cpx=p2.x
cpy=p2.y
if get_timer()>10 then
 spr(p2.sprite,p2.x*8,p2.y*8)
end
end
if player_state==1 then
  spr(p2.sprite,p2.x*8,p2.y*8)
 end
end

function try_move(xd,yd)
 
 newx+=xd
 newy+=yd
 p.x=mid(0,newx,127)
 p.y=mid(0,newy,63)
 p.moves-=1*tile_movement(newx,newy)
 
end

function try2_move(xd,yd)
 
 newx2+=xd
 newy2+=yd
 
 p2.x=mid(0,newx2,127)
 p2.y=mid(0,newy2,63)
 p2.moves-=1*tile_movement(newx2,newy2)
end

function player_1_state()
 newx=p.x
 newy=p.y
 
 if (p.moves<=0) then
  toggle_turn()
  return
 end
 if (btnp(‚¨ÖÔ∏è)) try_move(-1,0)
 if (btnp(‚û°Ô∏è)) try_move(1,0)
 if (btnp(‚¨ÜÔ∏è)) try_move(0,-1)
 if (btnp(‚¨áÔ∏è)) try_move(0,1)
 if (btnp(üÖæÔ∏è)) toggle_road()
 
-- interact(newx,newy)
 

end

function player_2_state()
 newx2=p2.x
 newy2=p2.y
 
 if (p2.moves<=0) then
  toggle_turn()
  return
 end
 
 if (btnp(‚¨ÖÔ∏è)) try2_move(-1,0)
 if (btnp(‚û°Ô∏è)) try2_move(1,0)
 if (btnp(‚¨ÜÔ∏è)) try2_move(0,-1)
 if (btnp(‚¨áÔ∏è)) try2_move(0,1)
 if (btnp(üÖæÔ∏è)) toggle_road()
 
 --if (btn(5)) player_state=1
 
-- interact(newx,newy)
 

end
