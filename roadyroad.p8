--map code

function map_setup()
road=0
space_berry=1
new_growth=2
grass=3
end

function draw_map()


-- map(0,0,0,0,16,16)
function draw_map()
 mapx=flr(cpx/16)*16
 mapy=flr(cpy/16)*16
 camera(mapx*8,mapy*8)

 map(0,0,0,0,128,64)
end

end

function is_tile(tile_type,x,y)
 tile=mget(x,y)
 has_flag=fget(tile,tile_type)
 return has_flag
end

function tile_movement(x,y)
 if(is_tile(road,x,y)) then
  return .3

 elseif(is_tile(space_berry,x,y)) then
   return 10
 
 else return 1
 end
 
end


function draw_current_player()
 if player_state==1 then
  circfill( p.x*8+4, p.y*8+4, 8,8)
 end
 if player_state==2 then
  circfill( p2.x*8+4, p2.y*8+4, 8,8)
 end
end

function toggle_road()
 if(is_tile(grass,cpx,cpy)) then
 --change back to grass
 mset(cpx,cpy,3)
 elseif(is_tile(road,cpx,cpy))then
 mset(cpx,cpy,1)
 end
 
end


